<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:ns1="http://schemas.android.com/tools">
    <color name="black">#000000</color>
    <color name="colorAccent">#FFAB40</color>
    <color name="colorPrimary">#FFC107</color>
    <color name="colorPrimaryDark">#FF7043</color>
    <color name="navigation_color_add">#8C8C8C</color>
    <color name="navigation_color_chart">#008577</color>
    <color name="navigation_color_habit">#E6A919</color>
    <color name="navigation_color_mood">#C9379D</color>
    <color name="navigation_color_profile">#1194AA</color>
    <color name="white">#FFFFFF</color>
    <string name="app_name">My Application</string>
    <style name="Theme.MyApplication" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/navigation_color_add</item>
        <item name="colorPrimaryVariant">@color/navigation_color_mood</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/navigation_color_habit</item>
        <item name="colorSecondaryVariant">@color/navigation_color_chart</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" ns1:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
</resources>